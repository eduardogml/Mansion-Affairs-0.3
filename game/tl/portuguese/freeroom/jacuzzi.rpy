# game/freeroom/jacuzzi.rpy:243
translate portuguese emma_fr1_ava_isabella_4bbc30e8:

    # "{i}Pretending that none of this mattered when, in reality, it mattered way too much.{/i}"
    "{i}Fingir que nada disso importava quando, na realidade, importava demais.{/i}"

# game/freeroom/jacuzzi.rpy:250
translate portuguese emma_fr1_ava_isabella_b910f056_1:

