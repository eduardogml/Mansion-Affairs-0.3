    old "{font=fonts/hatten.ttf} {size=50}I will intervene.{/size}{/font}"
    new "{font=fonts/hatten.ttf} {size=50}Я вмешаюсь.{/size}{/font}"

    # game/story_scripts/day4_alex.rpy:437
    old "{font=fonts/hatten.ttf} {size=50}I will observe, for now.{/size}{/font}"
    new "{font=fonts/hatten.ttf} {size=50}Я пока понаблюдаю.{/size}{/font}"

